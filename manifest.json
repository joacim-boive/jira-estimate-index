{
  "name": "JIRA Estimate Index",
  "version": "0.0.0.1",
  "manifest_version": 2,
  "description": "Review estimates compared to actual delivery",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "80": "icons/icon80.png",
    "128": "icons/icon128.png"
  },

  "browser_action": {
    "default_icon": "icons/icon19.png",
    "default_title": "JIRA Estimate Index",
    "default_popup": "src/browser_action/browser_action.html"
  },
  "permissions": [
    "tabs",
    "storage",
    "<all_urls>"
  ],
  "minimum_chrome_version": "55"
}